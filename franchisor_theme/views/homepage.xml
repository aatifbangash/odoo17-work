<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="franchisor_homepage" name="Record listing">
            <t t-call="website.layout">
                <div class="oe_structure"/>
                    <section class="mx-auto" style="margin-bottom: 5rem; margin-top: 5rem;">
                        <div class="container-fluid px-0">
                            <img class="w-100" t-att-src="'/franchisor_theme/static/img/schell-franchisor-home.png'"/>
                            <div class="division-card">
                                <p>
                                    FIND YOUR NEW HOME
                                <span class="text-center text-light">
                                    Beautifully designed with a unique approach.
                                </span>
                                </p>
                                <div class="text-center">
                                     <a t-attf-href="/find-your-home" class="btn btn-primary rounded-0 division-button">Find Your Home</a>
                                </div>
                            </div>
                        </div>
                    </section>
                    <section class="mission_section">
                        <div class="container">
                            <div class="row">
                                <div class="col-lg-6">
                                    <img class="w-100" t-att-src="'/franchisor_theme/static/img/schell-tricycle.png'"/>
                                </div>
                                <div class="col-lg-6">
                                    <div class="d-flex our_mission">
                                        <div class="heading">
                                            <h3>Our Mission of Happiness</h3>
                                        </div>
                                        <div class="description">
                                            <p>While we've earned a reputation for building distinctive homes,
                                                much of our company's success is attributed to our unique business
                                                purpose: to maximize happiness rather than profit! We're truly
                                                focused on our own happiness and the happiness of our customers.
                                            </p>
                                        </div>
                                        <div class="learn_btn_wrapper">
                                            <button class="btn btn-primary learn_more">Learn More</button>
                                        </div>
                                    </div>
                                    <div class="d-flex victory_images">
                                            <img class="w-100 h-100"
                                                 t-att-src="'/franchisor_theme/static/img/schell-victoryshake.png'"/>
                                            <img class="w-100"
                                                 t-att-src="'/franchisor_theme/static/img/schell-teamvictory.png'"/>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
